           " ", "\n", "                ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"ap", "[", 
              RowBox[{"[", "r", "]"}], "]"}], " ", "!=", " ", "0"}], ",", " ",
             "\n", "\t\t", 
            RowBox[{"s", " ", "=", " ", 
             RowBox[{"Join", "[", 
              RowBox[{"s", ",", " ", 
               RowBox[{"{", "x", "}"}]}], "]"}]}], ",", "\n", 
            "                       ", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"Or", " ", "@@", " ", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"en", "[", 
                    RowBox[{"#1", ",", " ", "s"}], "]"}], " ", "&"}], " ", 
                  ")"}], " ", "/@", " ", 
                 RowBox[{"(", 
                  RowBox[{"x", " ", "-", " ", "gen"}], ")"}]}]}], ")"}], ",", 
