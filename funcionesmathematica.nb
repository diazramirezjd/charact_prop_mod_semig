      RowBox[{"ap", " ", "=", " ", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "y", "}"}], ",", " ", 
         RowBox[{"Table", "[", 
          RowBox[{"0", ",", " ", 
           RowBox[{"{", 
            RowBox[{"y", " ", "-", " ", "1"}], "}"}]}], "]"}]}], "]"}]}], ";",
       "\n", "    ", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"en", "[", 
         RowBox[{"0", ",", " ", "ap"}], "]"}], ",", "\n", "     ", 
        RowBox[{"(", 
