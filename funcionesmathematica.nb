Cell["\<\
Devuelve el sistema minimal de generadores del semigrupo generado por {s}, \
sistemaminimal[cond],  da el sistema minimal de generadores del semigrupo \
determinado por la condici\[OAcute]n \[OpenCurlyDoubleQuote]cond\
\[CloseCurlyDoubleQuote]. 
Necesita de las funciones auxiliares apery y pertenece.\
\>", "Text",
 CellChangeTimes->{{3.580628761506718*^9, 3.5806288644200993`*^9}, {
  3.5806292290083394`*^9, 3.580629249444375*^9}, {3.587356272362008*^9, 
  3.5873562764215784`*^9}},
 FontFamily->"Lucida Sans",
 FontSize->16,
 FontColor->GrayLevel[0],
 Background->GrayLevel[1]],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"sistemaminimal", "[", 
    RowBox[{"{", "y__Integer", "}"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "x", "}"}], ",", 
     RowBox[{
      RowBox[{"x", "=", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"{", "y", "}"}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"#", "!=", "0"}], ")"}], "&"}]}], "]"}]}], ";", "\n", "  ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"x", "==", 
         RowBox[{"{", "}"}]}], ",", 
        RowBox[{"Return", "[", 
         RowBox[{"{", "}"}], "]"}]}], "]"}], ";", "\n", "  ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"GCD", "@@", "x"}], "==", "1"}], ",", " ", 
        RowBox[{"sistemaminimal2", "[", "x", "]"}], ",", 
        RowBox[{"sistemaminimal1", "[", "x", "]"}]}], "]"}]}]}], "]"}]}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"sistemaminimal1", "[", 
    RowBox[{"{", 
     RowBox[{"x___Integer", ",", " ", "y_Integer", ",", " ", "z___Integer"}], 
     "}"}], "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"sistemaminimal1", "[", 
     RowBox[{"{", 
      RowBox[{"x", ",", "\n", " ", "z"}], "}"}], "]"}], " ", "/;", 
    RowBox[{"pertenece", "[", 
     RowBox[{"y", ",", " ", 
      RowBox[{"{", 
       RowBox[{"x", ",", " ", "z"}], "}"}]}], "]"}]}]}], " ", "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"sistemaminimal1", "[", 
    RowBox[{"{", "x__Integer", "}"}], "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"{", "x", "}"}], " ", "//", "Sort"}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"sistemaminimal2", "[", 
   RowBox[{"{", "x__Integer", "}"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"m", ",", "ap"}], "}"}], ",", " ", 
     RowBox[{
      RowBox[{"m", "=", 
       RowBox[{"Min", "[", "x", "]"}]}], ";", " ", 
      RowBox[{"ap", "=", 
       RowBox[{
        RowBox[{"apery", "[", 
         RowBox[{"m", ",", 
          RowBox[{"{", "x", "}"}]}], "]"}], "/.", " ", 
        RowBox[{"0", "->", "m"}]}]}], ";", "\n", "  ", 
      RowBox[{"Select", "[", 
       RowBox[{"ap", ",", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"And", "@@", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Or", "[", 
               RowBox[{
                RowBox[{"Not", "[", 
                 RowBox[{"en", "[", 
                  RowBox[{"#", ",", "ap"}], "]"}], "]"}], ",", 
                RowBox[{"#", "==", "0"}]}], "]"}], "&"}], "/@", 
             RowBox[{"(", 
              RowBox[{"#", "-", "ap"}], ")"}]}], ")"}]}], ")"}], "&"}]}], 
       "]"}]}]}], "]"}], " ", "//", " ", "Sort"}]}]}], "Code",
 FontFamily->"Lucida Sans",
 FontSize->16,
 FontColor->GrayLevel[0],
 Background->GrayLevel[1]],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"sistemaminimal", "[", "cond_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "m", "}"}], ",", "\n", "  ", 
      RowBox[{
       RowBox[{"m", "=", "1"}], ";", " ", "\n", "  ", 
       RowBox[{"If", "[", 
