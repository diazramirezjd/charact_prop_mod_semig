Cell[BoxData[{
 RowBox[{
  RowBox[{"semiaux", "[", "cond_", "]"}], " ", ":=", "\n", 
  RowBox[{"Module", "[", "\n", "\t", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x", ",", " ", "s", ",", " ", "ap", ",", " ", "r", ",", " ", "m"}], 
     "}"}], ",", " ", "\n", "\t", 
    RowBox[{
     RowBox[{"x", " ", "=", " ", "1"}], ";", "\n", "\t", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"Not", "[", 
        RowBox[{"cond", "[", "x", "]"}], "]"}], ",", 
       RowBox[{"x", "++"}]}], "]"}], ";", "\n", "\t", 
     RowBox[{"m", "=", "x"}], ";", "\n", "\t", 
     RowBox[{"ap", " ", "=", " ", 
      RowBox[{"Join", "[", 
       RowBox[{
        RowBox[{"{", "m", "}"}], ",", " ", 
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", " ", 
          RowBox[{"{", 
           RowBox[{"m", " ", "-", " ", "1"}], "}"}]}], "]"}]}], "]"}]}], ";", 
     "\n", "\t", 
     RowBox[{"x", "++"}], ";", "\n", "\t", 
     RowBox[{"s", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "m"}], "}"}]}], ";", "\n", "\t", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"en", "[", 
        RowBox[{"0", ",", " ", "ap"}], "]"}], ",", "\n", "\t\t", 
       RowBox[{"(", "\n", "\t\t\t", 
        RowBox[{
         RowBox[{"r", " ", "=", " ", 
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"x", ",", " ", "m"}], "]"}], " ", "+", " ", "1"}]}], ";", 
         " ", "\n", "\t\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"cond", "[", "x", "]"}], ",", " ", 
           RowBox[{
            RowBox[{"s", "=", 
             RowBox[{"Join", "[", 
              RowBox[{"s", ",", " ", 
               RowBox[{"{", "x", "}"}]}], "]"}]}], ";", "\n", "\t\t\t", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"ap", "[", 
                RowBox[{"[", "r", "]"}], "]"}], "==", "0"}], ",", 
              RowBox[{
               RowBox[{"ap", "[", 
                RowBox[{"[", "r", "]"}], "]"}], " ", "=", " ", "x"}]}], 
             "]"}]}]}], "]"}], ";", "\n", "\t\t", 
         RowBox[{"x", "++"}], ";"}], "\n", "\t\t", ")"}]}], "\n", "\t", "]"}],
      ";", "\n", "\t", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"m", "==", "1"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], ",", "s"}], "]"}]}]}], "\n", 
   "]"}]}], "\n", 
